{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "templatewizard",
  "version": "0.1.0",
  "identifier": "com.templatewizard.app",  
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "resizable": false,
        "title": "Custom Api Template",
        "width": 400,
        "height": 550,
        "maximizable": false,
        "minimizable": false
      }
    ],
    "security": {
      "csp": null
    }    
  },  
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }  
}
